 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP OrUsingNand {
    IN x, y;
    OUT out;

    PARTS:
    Nand(a=x,b=x,out=o1);
	Nand(a=y,b=y,out=o2);
	Nand(a=o1,b=o2,out=out);
	
}
